@model IList<PollModel>
@if (Model.Count > 0)
{
    <div class="home-page-polls">
        <div class="title">
            <h1>@T("Polls.Title")</h1>
        </div>
        <div class="news-slider">
                 <div class="owl-carousel owl-carousel3 owl-theme">
            @foreach (var poll in Model)
            {
                @await Html.PartialAsync("_Poll", poll)
            }
        </div>
        </div>
    </div>
}
